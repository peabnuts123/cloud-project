{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-consistent-codestyle"
    ],
    "jsRules": {
      "ordered-imports": false,
      "interface-name": false,
      "object-literal-sort-keys": false,
      "max-line-length": false,
      "quotemark": false,
      "no-consecutive-blank-lines": false,
      "no-empty": false,
      "jsx-no-multiline-js": false
    },
    "rules": {
      "ordered-imports": false,
      "interface-name": false,
      "object-literal-sort-keys": false,
      "max-line-length": false,
      "quotemark": false,
      "no-consecutive-blank-lines": false,
      "member-ordering": false,
      "no-empty": false,
      "jsx-no-multiline-js": false
    }
}